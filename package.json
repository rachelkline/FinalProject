{
  "name": "group-travel",
  "version": "1.0.0",
  "description": "Group Travel Planner",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node scripts/seedDB.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@devexpress/dx-react-chart": "^2.7.3",
    "@devexpress/dx-react-chart-bootstrap4": "^2.7.3",
    "@devexpress/dx-react-chart-material-ui": "^2.7.3",
    "@devexpress/dx-react-core": "^2.7.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-regular-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.5.3",
    "classnames": "^2.2.6",
    "connect-mongo": "^3.2.0",
    "d3-scale-chromatic": "^2.0.0",
    "date-fns": "^2.16.1",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "foreman": "^3.0.1",
    "if-env": "^1.0.4",
    "lodash": "^4.17.20",
    "mongoose": "^5.10.13",
    "morgan": "^1.10.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "prop-types": "^15.7.2",
    "react-bootstrap": "^1.4.0",
    "react-bootstrap-sidebar": "0.0.1",
    "react-collapse": "^5.0.1",
    "react-datepicker": "^3.3.0",
    "react-hook-form": "^6.11.0",
    "react-router-dom": "^5.2.0",
    "react-sidebar": "^3.0.2"
  }
}
